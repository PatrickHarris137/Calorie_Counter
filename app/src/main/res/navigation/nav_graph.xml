<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/FirstFragment">

    <fragment
        android:id="@+id/FirstFragment"
        android:name="com.example.caloriecounter.DailyMacroCounter.DailyMacroCounterFragment"
        android:label="@string/first_fragment_label"
        tools:layout="@layout/fragment_daily_macro_counter">

        <action
            android:id="@+id/action_FirstFragment_to_SecondFragment"
            app:destination="@id/SecondFragment" />
    </fragment>
    <fragment
        android:id="@+id/SecondFragment"
        android:name="com.example.caloriecounter.DailyMacroCounter.DailyMacroCounter2Fragment"
        android:label="@string/second_fragment_label"
        tools:layout="@layout/fragment_second_daily_macro_counter">

        <action
            android:id="@+id/action_SecondFragment_to_FirstFragment"
            app:destination="@id/FirstFragment" />
    </fragment>
    <fragment
        android:id="@+id/FirstFragment"
        android:name="com.example.caloriecounter.FoodDisplay.FoodDisplayFragment"
        android:label="@string/first_fragment_label"
        tools:layout="@layout/fragment_food_display">

        <action
            android:id="@+id/action_FirstFragment_to_Second2Fragment"
            app:destination="@id/Second2Fragment" />
    </fragment>
    <fragment
        android:id="@+id/Second2Fragment"
        android:name="com.example.caloriecounter.FoodDisplay.FoodDisplay2Fragment"
        android:label="@string/second_fragment_label"
        tools:layout="@layout/fragment_second_fooddisplay">

        <action
            android:id="@+id/action_Second2Fragment_to_FirstFragment"
            app:destination="@id/FirstFragment" />
    </fragment>
    <fragment
        android:id="@+id/FirstFragment"
        android:name="com.example.caloriecounter.WeeklyProgression.WeeklyProgressionFragment"
        android:label="@string/first_fragment_label"
        tools:layout="@layout/fragment_weekly_progression">

        <action
            android:id="@+id/action_FirstFragment_to_SecondFragment"
            app:destination="@id/SecondFragment" />
    </fragment>
    <fragment
        android:id="@+id/SecondFragment"
        android:name="com.example.caloriecounter.WeeklyProgression.WeeklyProgressionSecondFragment"
        android:label="@string/second_fragment_label"
        tools:layout="@layout/fragment_second_weekly_progression">

        <action
            android:id="@+id/action_SecondFragment_to_FirstFragment"
            app:destination="@id/FirstFragment" />
    </fragment>
    <fragment
        android:id="@+id/FirstFragment"
        android:name="com.example.caloriecounter.DailyConsumption.DailyConsumptionFragment"
        android:label="@string/first_fragment_label"
        tools:layout="@layout/fragment_daily_consumption">

        <action
            android:id="@+id/action_FirstFragment_to_SecondFragment"
            app:destination="@id/SecondFragment" />
    </fragment>
    <fragment
        android:id="@+id/SecondFragment"
        android:name="com.example.caloriecounter.DailyConsumption.DailyConsumptionSecondFragment"
        android:label="@string/second_fragment_label"
        tools:layout="@layout/fragment_second_daily_consumption">

        <action
            android:id="@+id/action_SecondFragment_to_FirstFragment"
            app:destination="@id/FirstFragment" />
    </fragment>
</navigation>